{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "MAX_BODY_SIZE": "10mb"
    }
} 